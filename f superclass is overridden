class Superclass { 
public void show(){ 
System.out.println("This is the show() method of Superclass"); 
 } 
} 
 
class Subclass extends Superclass { 
    public void show()
{ 
 System.out.println("This is the show() method of Subclass"); 
 } 
} 
 public class MethodOverrideExample { public static void main(String[] args) { 
 Superclass superClassObj = new Superclass(); 
 Subclass subClassObj = new Subclass(); 
superClassObj.show(); 
subClassObj.show(); 
 } 
}
