// Java program to find compound interest  
import java.io.*; 

class Rohit
{ 
	public static void main(String args[]) 
	{ 
		double principal = 1200, rate = 5.4, time = 2; 

		double CI = principal * 
					(Math.pow((1 + rate / 100), time)); 
		
		System.out.println("Compound Interest is : "+ CI); 
	} 
} 
